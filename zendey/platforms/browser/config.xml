<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="1" id="com.zendey.flashcard" ios-CFBundleVersion="0.0.1" version="0.0.1" windows-packageVersion="0.0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
